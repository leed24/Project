public class project2 {
    private String name;
    private double eprice2;
    private int index2;

public project2(String namearrays, double eprice, int index) {
    name=namearrays;
     eprice2=eprice;
     index2=index;   
}

public void calculate(){
    
    double monthly=(index2*eprice2);
  double  yearly=monthly*12;
  System.out.print("The monthly price for "+name+" energy usage is: ");
  System.out.printf("'%5.2f'%n", monthly);
  System.out.print("The yearly price for "+name+" energy usage is: ");
  System.out.printf("'%5.2f'%n", yearly);
}

public String getname(){
  return name;
}
public double geteprice2(){
  return eprice2;
}
public int index2(){
  return index2;
}
public void setName(String newName){
  name =newName;
}
public void setEprice2(double newEprice2){
  eprice2= newEprice2;
}
public void setIndex2(int newIndex2){
  index2=newIndex2;
}
}
